import java.io.*;
import java.util.Scanner;

public class FileIO {

	public String readFile(String filename) {
		
		FileReader reader;
		
		try {
			
			reader = new FileReader(filename);
			Scanner in = new Scanner(reader);
			
			String input = in.nextLine();
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		
	}
}
