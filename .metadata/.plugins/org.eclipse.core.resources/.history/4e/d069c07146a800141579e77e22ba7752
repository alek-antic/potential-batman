	public Movie lineToMovie(String line) {
		String sorted[] = line.split("\\|");
		for(String s : sorted) {
			s.replaceAll("|", "");
		}
		int id = Integer.parseInt(sorted[0]);
		String title = sorted[1];
		String rD = sorted[2];
		String vRD = sorted[3];
		String url = sorted[4];
		
		
		return null;
	}
	
