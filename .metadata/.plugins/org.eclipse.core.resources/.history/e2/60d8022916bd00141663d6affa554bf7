package BigRealEstate;

public class Plot {
	
	private String name;
	private int price, width, length;
	
	public Plot(String name, int price ,int width, int length) {
		this.name = name;
		this.price = price;
		this.width = width;
		this.length = length;
	}
	
	public int getArea() {
		return width * length;
	}
	
	
	public String toString() {
		return name;
	}

}
