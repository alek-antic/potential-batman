package simschool.usables;

import java.io.*;
import java.util.Scanner;

public class FileIO {

	
	
	public static String readFile(String filename) {
		
		FileReader reader = null;
		Scanner in = null;
		
		
		try {
			reader = new FileReader(filename);
			in = new Scanner(reader);
		} catch(FileNotFoundException ex) {
			ex.printStackTrace();
		} finally {
			if(reader != null) {
				in.close();
			}
		}
		
		
		return null;
	}
}
